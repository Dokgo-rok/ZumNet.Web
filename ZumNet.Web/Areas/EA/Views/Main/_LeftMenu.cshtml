@using System.Data;

@{

}
<ul class="sidenav-inner">
    <li class="sidenav-item" data-box="node.do" location="do" actrole="" partid="@Session["URID"]">
        <a href="javascript:void(0)" class="sidenav-link font-weight-bold"><i class="fe-check-square sidenav-icon"></i><span>@Resources.EA.BoxToDo</span><span class="z-lm-cnt"></span></a>
    </li>
    <li class="sidenav-item active open">
        <a href="javascript:void(0)" class="sidenav-link font-weight-bold sidenav-toggle"><i class="fe-user-check sidenav-icon"></i><span>@Session["URName"] @Resources.EA.Box</span></a>

        <ul class="sidenav-menu my-0">
            <li class="sidenav-item font-weight-bold" data-box="node.ep" location="ep" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxExpected</span><span class="z-lm-cnt"></span></a>
            </li>
            @if (Session["FlowSvcMode"] != null && Session["FlowSvcMode"].ToString().Substring(0, 1) == "Y")
            {
                <li class="sidenav-item font-weight-bold" data-box="node.wt" location="wt" actrole="" partid="@Session["URID"]">
                    <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxWaiting</span><span class="z-lm-cnt"></span></a>
                </li>
            }
            <li class="sidenav-item font-weight-bold active" data-box="node.av" location="av" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxToApprove</span><span class="z-lm-cnt"></span></a>
            </li>
            <li class="sidenav-item font-weight-bold" data-box="node.go" location="go" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxInProcess</span><span class="z-lm-cnt"></span></a>
            </li>
            <li class="sidenav-item" data-box="node.cf" location="cf" actrole="cf" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxToConfirm</span><span class="z-lm-cnt"></span></a>
            </li>
            <li class="sidenav-item" data-box="node.dl" location="dl" actrole="_dl" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxDistribued</span><span class="z-lm-cnt"></span></a>
            </li>
            <li class="sidenav-item" data-box="node.cm" location="cm" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxCompleted</span></a>
            </li>
            <li class="sidenav-item" data-box="node.rj" location="rj" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxRejected</span><span class="z-lm-cnt"></span></a>
            </li>
            <li class="sidenav-item" data-box="node.ps" location="ps" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxViewLater</span></a>
            </li>
            <li class="sidenav-item" data-box="node.re" location="re" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxReference</span><span class="z-lm-cnt"></span></a>
            </li>
            <li class="sidenav-item" data-box="node.wd" location="wd" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxWithdrawn</span></a>
            </li>
            <li class="sidenav-item" data-box="node.te" location="te" actrole="" partid="@Session["URID"]">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxSaved</span></a>
            </li>
        </ul>
    </li>
    <li class="sidenav-item active open">
        <a href="javascript:void(0)" class="sidenav-link font-weight-bold sidenav-toggle"><i class="fe-users sidenav-icon"></i><span>@Session["DeptName"] @Resources.EA.Box</span></a>

        <ul class="sidenav-menu my-0">
            @if (ViewBag.FormCharger)
            {
                <li class="sidenav-item font-weight-bold" data-box="node.as" location="as" actrole="__r" partid="@(Session["DeptID"])__r">
                    <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxAssigned</span><span class="z-lm-cnt"></span></a>
                </li>
            }
            @if (ViewBag.RcvManager || ViewBag.R.current["operator"].ToString() == "Y")
            {
                <li class="sidenav-item font-weight-bold" data-box="node.__r" location="__r" actrole="__r" partid="@(Session["DeptID"])__r">
                    <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxReceived</span><span class="z-lm-cnt"></span></a>
                </li>
            }
            <li class="sidenav-item" data-box="node._s" location="_s" actrole="_s" partid="@(Session["DeptID"])_s">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxSent</span></a>
            </li>
            <li class="sidenav-item" data-box="node._a" location="_a" actrole="_a" partid="@(Session["DeptID"])_a">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxCompleted</span></a>
            </li>
            <li class="sidenav-item" data-box="node._re" location="_re" actrole="_re" partid="@(Session["DeptID"])_re">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxReference</span><span class="z-lm-cnt"></span></a>
            </li>
            <li class="sidenav-item" data-box="node._dl" location="dl" actrole="_dl" partid="@(Session["DeptID"])_dl">
                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxDistribued</span><span class="z-lm-cnt"></span></a>
            </li>
        </ul>
    </li>
    @if (ViewBag.SharedFolder != null && ViewBag.SharedFolder.Count > 0)
    {
        <li class="sidenav-item active open">
            <a href="javascript:void(0)" class="sidenav-link font-weight-bold sidenav-toggle"><i class="fab fa-chromecast sidenav-icon text-pink"></i><span>@Resources.EA.BoxShared</span></a>

            <ul class="sidenav-menu my-0">
                @foreach (DataRow row in ViewBag.SharedFolder)
                {
                    <li class="sidenav-item">
                        <a href="javascript:void(0)" class="sidenav-link sidenav-toggle"><i class="far fa-folder sidenav-icon text-secondary"></i><span>@row["TargetGR"]</span></a>

                        <ul class="sidenav-menu my-0">
                            <li class="sidenav-item" data-box="node._s" location="_s" actrole="_s" partid="@(row["TargetGRID"])_s">
                                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxSent</span></a>
                            </li>
                            <li class="sidenav-item" data-box="node._a" location="_a" actrole="_a" partid="@(row["TargetGRID"])_a">
                                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxCompleted</span></a>
                            </li>
                            <li class="sidenav-item" data-box="node._re" location="_re" actrole="_re" partid="@(row["TargetGRID"])_re">
                                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxReference</span></a>
                            </li>
                            <li class="sidenav-item" data-box="node._dl" location="dl" actrole="_dl" partid="@(row["TargetGRID"])_dl">
                                <a href="javascript:void(0)" class="sidenav-link"><span>@Resources.EA.BoxDistribued</span></a>
                            </li>
                        </ul>
                    </li>
                }
            </ul>
        </li>
    }

</ul>


