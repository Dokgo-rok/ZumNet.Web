                            {
                                if (svcRt.ResultDataSet.Tables.Count == 3 && svcRt.ResultDataSet.Tables[2].Rows.Count > 0)
                                {
                                    row = svcRt.ResultDataSet.Tables[2].Rows[0];
                                    strToolCnt = StringHelper.SafeString(row["금형보유건수"], "0");
                                }
                            }
