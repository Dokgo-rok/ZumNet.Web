                            vParam[0] = StringHelper.SafeString(row["CONV_PREDICT"], "0");
                            vParam[1] = row["CORRENCY"].ToString();
                            vParam[2] = StringHelper.SafeString(row["PREDICT"], "0");
                            vParam[3] = StringHelper.SafeString(row["CONVRATE"], "0");
