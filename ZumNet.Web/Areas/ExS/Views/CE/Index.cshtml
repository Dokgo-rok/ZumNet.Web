            if (ViewBag.Mode != "new" && ViewBag.Mode != "edit") if (row["LNKEA"].ToString() != "" && row["LNKEA"].ToString() != "0") sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" onclick=\"_zw.fn.openEAFormSimple('{0}');\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-tag\"></i> {1}</a></li>", row["LNKEA"].ToString(), "관련문서");
