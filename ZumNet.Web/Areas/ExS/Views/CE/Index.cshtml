@functions{
    private string GetQueryString(string m, string page, string ttl, string appId)
    {
        return "?qi=" + SecurityHelper.ToBase64("{M:\"" + m + "\",ct:\"" + ViewBag.R.ct + "\",ctalias:\"" + ViewBag.R.ctalias + "\",ttl:\"" + ttl + "\",opnode:\"\",ft:\"" + page + "\",appid:\"" + appId + "\"}");
    }

    private string RenderMenu(string formTable, DataSet ds)
    {
        DataRow row = null;
        if (ds != null && ds.Tables.Count > 0 && ds.Tables[0].Rows.Count > 0) row = ds.Tables[0].Rows[0];

        StringBuilder sb = new StringBuilder();

        if (formTable == "Grid")
        {
            sb.Append("<li class=\"nav-item\"><button class=\"btn icon-btn btn-outline-secondary m-0 btn-sm\" aria-controls=\"vw-full-fill\" onclick=\"_zw.ut.ctrls();\"><i class=\"fas fa-expand\"></i></button></li>");
        }
        else if (formTable == "MyList" || formTable == "DeptList" || formTable == "List" || formTable == "Report")
        {
            if (formTable == "Report" && (ViewBag.R.current["operator"].ToString() == "Y" || StringHelper.HasAcl(ViewBag.R.current["acl"].ToString(), "M")))
            {
                sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"text-muted\" disabled title=\"{0}\" data-zf-menu=\"reportGrid\"><i class=\"fe-edit\"></i> {0}</a></li>", "집계견적표작성");
                sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"text-muted\" disabled title=\"{0}\" data-zf-menu=\"reqApproval\"><i class=\"fas fa-paper-plane\"></i> {0}</a></li>", "결재요청");
            }
            else if (formTable == "DeptList")
            {
                sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"text-muted\" disabled title=\"{0}\" data-zf-menu=\"viewChiefCfm\"><i class=\"fas fa-check\"></i> {0}</a></li>", "확인");
            }
            else if (formTable == "MyList")
            {
                sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"text-muted\" disabled title=\"{0}\" data-zf-menu=\"copyGrid\"><i class=\"fe-copy\"></i> {0}</a></li>", "복사하기");
            }

            sb.Append("<li class=\"nav-item\"><button class=\"btn icon-btn btn-outline-secondary m-0 btn-sm\" aria-controls=\"vw-full-fill\" onclick=\"_zw.ut.ctrls();\"><i class=\"fas fa-expand\"></i></button></li>");
        }
        else if (formTable == "StdExchangeDetail")
        {
            sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-backward\"></i> {2}</a></li>", GetQueryString("", "StdExchange", "", "0"), "되돌아가기", Resources.Global.List);

            if (ViewBag.R.current["operator"].ToString() == "Y" || StringHelper.HasAcl(ViewBag.R.current["acl"].ToString(), "S"))
            {
                if (ViewBag.Mode == "new" || ViewBag.Mode == "edit")
                {
                    sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"saveStd\"><i class=\"fas fa-check\"></i> {0}</a></li>", Resources.Global.Save, "환율 저장하기");
                    //if (Master.Mode == "edit") sb.AppendFormat("<li><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"reqApproval\"><i class=\"glyphicon glyphicon-send\"></i> 결재요청</a></li>", "결재요청하기");
                }
                else
