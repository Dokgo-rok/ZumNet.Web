            {
                if (ViewBag.Mode == "new" || ViewBag.Mode == "reuse" || ViewBag.Mode == "edit")
                {
                    sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"saveStd\"><i class=\"fas fa-check\"></i> {0}</a></li>", Resources.Global.Save, "기준임율 저장하기");
                    //if (Master.Mode == "edit") sb.AppendFormat("<li><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"reqApproval\"><i class=\"glyphicon glyphicon-send\"></i> 결재요청</a></li>", "결재요청하기");
                }
                else
                {
                    if (row != null && row["STATE"].ToString() == "0" && (ViewBag.R.current["operator"].ToString() == "Y" || Session["URID"].ToString() == row["CRID"].ToString())) //관리자, 작성자
                    {
                        sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-check-square\"></i> {2}</a></li>", GetQueryString("edit", formTable, "", ViewBag.R.appid.ToString()), "기준임율 편집하기", Resources.Global.Edit);
                        sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"reqApproval\"><i class=\"fas fa-paper-plane\"></i> 결재요청</a></li>", "결재요청하기");
                    }
                    if (row["STATE"].ToString() == "7") sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-edit\"></i> 임율작성</a></li>", GetQueryString("new", formTable, "", "0"), "기준임율 작성하기");
                    else if (row["STATE"].ToString() == "8" || row["STATE"].ToString() == "6") sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n text-danger\" title=\"{1}\"><i class=\"fas fa-copy\"></i> 임율재사용</a></li>", GetQueryString("reuse", formTable, "", ViewBag.R.appid.ToString()), "임율 재사용하기");
                }

                if (row != null && row["STATE"].ToString() == "0" && (ViewBag.R.current["operator"].ToString() == "Y" || Session["URID"].ToString() == row["CRID"].ToString())) //관리자, 작성자
                {
                    sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"deleteStd\"><i class=\"fe-x\"></i> 삭제</a></li>", "삭제하기");
                }
            }

            if (ViewBag.Mode != "new" && ViewBag.Mode != "reuse" && ViewBag.Mode != "edit") if (row["LNKEA"].ToString() != "" && row["LNKEA"].ToString() != "0") sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" onclick=\"_zw.fn.openEAFormSimple('{0}');\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-tag\"></i> {1}</a></li>", row["LNKEA"].ToString(), "관련문서");
        }
        else if (formTable == "OutPayDetail")
        {
            sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-backward\"></i> {2}</a></li>", GetQueryString("", "OutPay", "", "0"), "되돌아가기", Resources.Global.List);

            if (ViewBag.R.current["operator"].ToString() == "Y" || StringHelper.HasAcl(ViewBag.R.current["acl"].ToString(), "S"))
            {
                if (ViewBag.Mode == "new" || ViewBag.Mode == "edit")
                {
                    sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"saveStd\"><i class=\"fas fa-check\"></i> {0}</a></li>", Resources.Global.Save, "외주임율 저장하기");
                }
                else
                {
                    if (row != null && row["STATE"].ToString() == "0" && (ViewBag.R.current["operator"].ToString() == "Y" || Session["URID"].ToString() == row["CRID"].ToString())) //관리자, 작성자
                    {
                        sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-check-square\"></i> {2}</a></li>", GetQueryString("edit", formTable, "", ViewBag.R.appid.ToString()), "외주임율 편집하기", Resources.Global.Edit);
                    }
                    if (row["STATE"].ToString() == "7" || row["STATE"].ToString() == "8" || row["STATE"].ToString() == "6") sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-edit\"></i> 임율작성</a></li>", GetQueryString("new", formTable, "", "0"), "외주임율 작성하기");
                }

                if (row != null && row["STATE"].ToString() == "0" && (ViewBag.R.current["operator"].ToString() == "Y" || Session["URID"].ToString() == row["CRID"].ToString())) //관리자, 작성자
                {
                    sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"deleteStd\"><i class=\"fe-x\"></i> 삭제</a></li>", "삭제하기");
                    sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"javascript:\" class=\"z-lnk-navy-n\" title=\"{0}\" data-zf-menu=\"changeState\"><i class=\"fas fa-forward\"></i> 완료처리</a></li>", "완료 처리하기");
                }
