<!-- `.messages-wrapper` fills all available space of container -->
<div class="messages-wrapper">

    <!-- sidebox -->
    @Html.Partial("_LeftMenu")
    <!-- / sidebox -->
    <!-- content -->
    <div class="d-flex flex-column w-100 bg-white" data-controls="vw-full-fill">
        <!-- Header -->
        <div class="flex-grow-0">
            <div class="d-flex justify-content-between container-p-x m-0 mt-1 pt-3" style="padding-bottom: .65rem !important">
                <div class="d-flex align-items-center font-weight-bold">
                    <div class="h5 mb-0">
                        <a href="javascript:void(0)" class="messages-sidebox-toggler d-none d-md-block d-lg-none align-self-center px-2 mr-3" data-toggle="modal" data-target=".messages-sidebox"><i class="fe-more-vertical"></i></a>
                    </div>

                    <div class="h5 mb-0 z-ttl py-0 text-truncate">
                        <i class="mdi mdi-file-table-box-multiple-outline"></i> &nbsp;<span>@ViewBag.Title</span>
                    </div>
                </div>
                <ul class="nav d-flex align-items-center justify-content-end z-lv-menu">
                    @Html.Raw(RenderMenu(ViewBag.R.ft.ToString(), ViewBag.BoardList))
                </ul>
            </div>
        </div>

        <div class="flex-grow-0" style="border-top: 2px solid #eee">
            @if (ViewBag.R.ft.ToString() == "MyList" || ViewBag.R.ft.ToString() == "DeptList" || ViewBag.R.ft.ToString() == "List" || ViewBag.R.ft.ToString() == "Report")
            {
                <div class="d-flex justify-content-between px-3 my-2">
                    @Html.Raw(RenderSearch(ViewBag.R.ft.ToString()))
                </div>
            }
        </div>
        <!-- / Header -->

        <div class="flex-grow-1 position-relative">
            @if (ViewBag.R.ft.ToString() == "_MyList" || ViewBag.R.ft.ToString() == "_DeptList" || ViewBag.R.ft.ToString() == "_List" || ViewBag.R.ft.ToString() == "_Report")
            {
                <div class="z-list z-list-sticky" id="__List">
                    @Html.Partial(sPatialView)
                </div>
            }
            else
            {
                <div class="px-3@(sListScroll)">
                    <div class="z-list">
                        <div class="z-list-head"></div>
                        <div class="z-list-body" id="__List">
                            @Html.Partial(sPatialView)
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    <!-- / content -->

    @if (ViewBag.R.ft.ToString() == "StdExchangeDetail")
    {
        <div class="d-none" id="_HiddenForm">
            <div style="padding-left: 4px; font-size: 18px; font-weight: 700">&#9635; 기존 및 당월 예상 기준 환율</div>
            <div>
