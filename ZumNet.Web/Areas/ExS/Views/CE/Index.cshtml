                    if (row != null && row["STATE"].ToString() == "0" && (ViewBag.R.current["operator"].ToString() == "Y" || Session["URID"].ToString() == row["CRID"].ToString())) //관리자, 작성자
                    {
                        sb.AppendFormat("<li class=\"nav-item mr-3\"><a href=\"{0}\" class=\"z-lnk-navy-n\" title=\"{1}\"><i class=\"fas fa-check-square\"></i> {2}</a></li>", GetQueryString("edit", formTable, "", ViewBag.R.appid.ToString()), "환율 편집하기", Resources.Global.Edit);
