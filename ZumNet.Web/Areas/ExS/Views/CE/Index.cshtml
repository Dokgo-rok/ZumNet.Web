        return sb.ToString();
    }

    private string RenderSearch(string formTable)
    {
        string sSelected = "";

        StringBuilder sb = new StringBuilder();

        sb.Append("<div class=\"z-list-page d-flex align-items-center\">");
        sb.Append("<i class=\"fe-clock mr-2\"></i>");
        sb.Append("<div class=\"z-list-cnt mr-3\">");
        sb.Append("<select class=\"custom-select\" id=\"_SearchYear\">");
        sb.Append("<option value=\"ALL\">전체</option>");
        sb.AppendFormat("<option value=\"1\"{0}>이전 1년</option>", StringHelper.SafeString(ViewBag.R.lv["start"]) == "1" ? " selected" : "");
        for (int i = DateTime.Now.Year; i >= 2020; i--)
        {
            sb.AppendFormat("<option value=\"{0}\"{1}>{0}년</option>", i.ToString(), StringHelper.SafeString(ViewBag.R.lv["start"]) == i.ToString() ? " selected" : "");
        }
        sb.Append("</select>");
        sb.Append("</div>");

        if (formTable == "MyList")
        {
            sSelected = StringHelper.SafeString(ViewBag.R.lv["cd1"]) == "9" ? " checked" : "";
            sb.Append("<label class=\"custom-control custom-radio m-0 mr-2\" for=\"rdo01\">");
            sb.AppendFormat("<input type=\"radio\" class=\"custom-control-input\" name=\"rdoSearch\" id=\"rdo01\" value=\"9\"{0}>", sSelected);
            sb.Append("<span class=\"custom-control-label\">임시 저장</span>");
            sb.Append("</label>");

            sSelected = StringHelper.SafeString(ViewBag.R.lv["cd1"]) == "" ? " checked" : "";
            sb.Append("<label class=\"custom-control custom-radio m-0 mr-2\" for=\"rdo02\">");
            sb.AppendFormat("<input type=\"radio\" class=\"custom-control-input\" name=\"rdoSearch\" id=\"rdo02\" value=\"\"{0}>", sSelected);
            sb.Append("<span class=\"custom-control-label\">전체</span>");
            sb.Append("</label>");
        }
        else if (formTable == "DeptList")
        {
            sSelected = StringHelper.SafeString(ViewBag.R.lv["cd1"]) == "Y" ? " checked" : "";
            sb.Append("<label class=\"custom-control custom-radio m-0 mr-2\" for=\"rdo01\">");
            sb.AppendFormat("<input type=\"radio\" class=\"custom-control-input\" name=\"rdoSearch\" id=\"rdo01\" value=\"Y\"{0}>", sSelected);
            sb.Append("<span class=\"custom-control-label\">확인 대상</span>");
            sb.Append("</label>");

            sSelected = StringHelper.SafeString(ViewBag.R.lv["cd1"]) == "" ? " checked" : "";
            sb.Append("<label class=\"custom-control custom-radio m-0 mr-2\" for=\"rdo02\">");
            sb.AppendFormat("<input type=\"radio\" class=\"custom-control-input\" name=\"rdoSearch\" id=\"rdo02\" value=\"\"{0}>", sSelected);
            sb.Append("<span class=\"custom-control-label\">전체</span>");
            sb.Append("</label>");
        }

        sb.Append("</div>");

        sb.Append("<div class=\"flex-fill z-lv-cond d-flex\">");
        sb.Append("<div class=\"input-group justify-content-end\">");

        if (formTable == "MyList" || formTable == "DeptList" || formTable == "List" || formTable == "Report")
        {
            sb.Append("<div class=\"input-group-prepend\">");
            sb.AppendFormat("<span class=\"input-group-text bg-light\">{0}</span>", "모델 + 고객");
            sb.Append("</div>");
            sb.AppendFormat("<input type=\"text\" class=\"form-control search-text\" placeholder=\"{0}\" style=\"max-width: 150px\" value=\"{1}\">", Resources.Global.Search, StringHelper.SafeString(ViewBag.R.lv["searchtext"]));
            sb.Append("<div class=\"input-group-append\">");
            sb.Append("<button class=\"btn btn-primary\" data-zv-menu=\"search\"><i class=\"fe-search\"></i></button>");
            sb.Append("</div>");
        }
        else if (formTable == "ExcelList")
        {
        }

        sb.Append("</div>");
        sb.Append("</div>");

        return sb.ToString();
    }
}
@section Styles {
    @Styles.Render("~/bundle/vendor/css/bootstrap-datepicker/bootstrap-datepicker")
    @Styles.Render("~/bundle/vendor/css/pages/messages")
    @Styles.Render("~/Content/ce.css")
}

@section Scripts {
    @Scripts.Render("~/bundle/vendor/js/bootstrap-datepicker/bootstrap-datepicker")
    @Scripts.Render("~/bundle/vendor/js/bootstrap-maxlength/bootstrap-maxlength")
    @Scripts.Render("~/bundle/vendor/js/vanilla-text-mask/vanilla-text-mask")
    @Scripts.Render("~/bundle/vendor/js/vanilla-text-mask/text-mask-addons")

    @Scripts.Render("~/Scripts/ExS/ce.js")

    @if (sCurrentLocale != "" && sCurrentLocale.Substring(0, 2) != "en")
    {
        <script src="/vendor/libs/bootstrap-datepicker/bootstrap-datepicker.@(sCurrentLocale.Substring(0, 2)).js" charset="UTF-8"></script>
    }

    <script type="text/javascript">(function () {_zw.V = @Html.Raw(JsonConvert.SerializeObject(ViewBag.R))}());</script>

    @if (ViewBag.R.ft.ToString() == "CodeMgr")
    {
        <script type="text/javascript">
            $(function () {
                $('.card[data-zf-code]').each(function () {
                    var $this = $(this), cd = $this.attr('data-zf-code'), tp = $this.find('.zf-sp-template');

                    $this.find('button[data-btn="add"]').on('click', function () {
                        $this.find('table tbody').append(tp.html());

                        var row = $this.find('table tbody tr').last();
                        row.find('button[data-btn="del"]').on('click', function () {
                            _zw.fn.setCode('delete', $this, cd, row);
                        });
                        row.find('button[data-btn="save"]').on('click', function () {
                            _zw.fn.setCode('save', $this, cd, row);
                        });
                    });

                    $this.find('button[data-btn="del"]').on('click', function () {
                        _zw.fn.setCode('delete', $this, cd, $(this).parent().parent().parent());
                    });

                    $this.find('button[data-btn="save"]').on('click', function () {
                        _zw.fn.setCode('save', $this, cd, $(this).parent().parent().parent());
                    });
                });

            });
        </script>

    }
    else if (ViewBag.R.ft.ToString() == "StdExchangeDetail" || ViewBag.R.ft.ToString() == "StdPayDetail" || ViewBag.R.ft.ToString() == "OutPayDetail")
    {
        <script type="text/javascript">
            $(function () {
                if (_zw.V.mode == 'new' || _zw.V.mode == 'reuse' || _zw.V.mode == 'edit') _zw.fn.input();
            });
        </script>
    }
}

