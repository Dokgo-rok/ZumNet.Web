<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <link rel="icon" type="image/x-icon" href="~/favicon.ico">

    <title>@ViewBag.Title</title>

    @Styles.Render("~/bundle/vendor/fonts")

    @Styles.Render("~/bundle/vendor/fonts/fontawesome")
    @Styles.Render("~/bundle/vendor/fonts/materialdesignicons")

    <!-- Core stylesheets -->
    @Styles.Render("~/bundle/vendor/css/bootstrap")
    @Styles.Render("~/bundle/css/theme-cresyn")
    

    @RenderSection("styles", required: false)
</head>
<body>
    @RenderBody()

    <!-- Core scripts -->
    @Scripts.Render("~/bundle/jquery")

    @RenderSection("scripts", required: false)
</body>
</html>
