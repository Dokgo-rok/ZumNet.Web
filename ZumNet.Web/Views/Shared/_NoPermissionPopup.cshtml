@{
    ViewBag.Title = "Not Permission";
    Layout = "~/Views/Shared/Layouts/_LayoutNone.cshtml";
}

<div class="my-5">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-8 col-lg-6 col-xl-4">
                <div class="card">
                    <div class="card-body p-4">
                        <div class="text-center text-danger my-3">
                            <h3 class="text-uppercase fw-bold">@(Resources.Global.Auth_NoPermission)!!</h3>
                        </div>
                        <div class="text-center">

                            <p class="text-muted mb-0 mt-3" style="line-height: 20px;">
                                Access Denied
                            </p>

                            <button class="btn btn-primary mt-3" onclick="window.close();"><i class="fe-log-out"></i> @Resources.Global.Close</button>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>
