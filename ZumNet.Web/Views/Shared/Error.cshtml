@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/Layouts/_LayoutBlank.cshtml";
}

@section Styles {
    <!-- Page -->
    @Styles.Render("~/bundle/vendor/css/pages/authentication")
}


<!--
=================================================================
**************************** CONTENT ****************************
-->

<div class="authentication-wrapper authentication-2 px-4">
    <div class="authentication-inner py-5">

        <!-- Form -->
        <form class="card">
            <div class="p-4 p-sm-5">

                <!-- Logo -->
                <div class="d-flex justify-content-center align-items-center pb-2 mb-4">
                    <div class="ui-w-60">
                        <div class="w-100 position-relative" style="padding-bottom: 54%">
                            <img src="/images/logo_cresyn.png" alt="" height="32">
                        </div>
                    </div>
                </div>
                <!-- / Logo -->

                <h4 class="text-center text-muted font-weight-normal mb-4">Application Error</h4>

                <hr class="mt-0 mb-4">

                <p>
                    오류 발생
                </p>

                <button type="button" class="btn btn-primary btn-block">이전</button>

            </div>
        </form>
        <!-- / Form -->

    </div>
</div>