@{
    ViewBag.Title = "위조 방지";
    Layout = "~/Views/Shared/Layouts/_LayoutNone.cshtml";
}

<div class="my-5">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-8 col-lg-6 col-xl-4">
                <div class="card">
                    <div class="card-body p-4">
                        <div class="text-center mt-3 mb-5">
                            <h3 class="text-uppercase fw-bold">@ViewBag.Title</h3>
                        </div>
                        <div class="text-center">
                            <h4 class="text-muted">@Resources.Global.Auth_AlreadyLogin</h4>
                            <a class="btn btn-primary mt-4" href="/"><i class="mdi mdi-home me-1"></i> Return Home</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

